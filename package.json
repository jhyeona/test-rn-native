{
  "name": "ihereapp",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "watchman watch-del-all && npx react-native start --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "android:ac": "react-native run-android --mode=appcenterRelease --appIdSuffix appcenter",
    "android:dev": "react-native run-android --mode=developmentdebug --appIdSuffix development",
    "android:prod": "react-native run-android --mode=productionRelease",
    "ios:ac": "react-native run-ios --scheme ihereapp_Appcenter",
    "ios:dev": "react-native run-ios --scheme ihereapp_Debug --simulator 'iPhone 13 mini'",
    "ios:prod": "react-native run-ios --scheme ihereapp --simulator 'iPhone 13 mini'"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-firebase/analytics": "^21.0.0",
    "@react-native-firebase/app": "^21.0.0",
    "@react-native-firebase/crashlytics": "^21.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@tanstack/react-query": "^5.56.2",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-native": "0.76.1",
    "react-native-bootsplash": "^6.1.3",
    "react-native-config": "^1.5.3",
    "react-native-device-info": "^13.0.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-mmkv": "^3.1.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-onesignal": "^5.2.4",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-webview": "^13.12.2",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@types/react": "^18.3.10",
    "@types/react-test-renderer": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.6.2"
  },
  "codegenConfig": {
    "name": "NativeBaseInfo",
    "type": "modules",
    "jsSrcsDir": "src/specs",
    "android": {
      "javaPackageName": "com.nativebaseinfo"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.5.0"
}
